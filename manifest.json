{
	"manifest_version": 2,
	"name": "Chrome App Bug",
	"version": "0.0.1",
	"minimum_chrome_version": "40",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": [
		"alwaysOnTopWindows"
	],
	"commands": {
		"toggle": {
			"suggested_key": {
				"default": "Ctrl+Space"
			},
			"description": "Toggle Window",
			"global": true
		}
	}
}
